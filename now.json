{
	"version": 2,
	"name": "falebonito",
	"alias": "falebonito.now.sh",
	"builds": [
		{
			"src": "package.json",
			"use": "@now/static-build"
		}
	],
	"routes": [
		{
			"src": "^/static/(.*)",
			"dest": "/static/$1"
		},
		{
			"src": "^/favicon.ico",
			"dest": "/favicon.ico"
		},
		{
			"src": "^/(.*)",
			"dest": "/index.html"
		}
	]
}